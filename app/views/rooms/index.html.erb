<h2> Rooms</h2>
<table class="table table-hover">
  <thead>
    <tr>
      <th> ID </th>
      <th> Price(VND) </th>
      <th> Occupants</th>
    </tr>
  </thead>
  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td> <%= room.name %></td>
        <td> <%= number_with_delimiter(room.price, delimiter: ",") %></td>
        <td> <%= room.occupants %></td>
        <td> </td>
      </tr>
    <% end %>
  </tbody>
</table>
